    {
      "cell_type": "code",
      "source": [
        "%%writefile serializers.py\n",
        "from rest_framework import serializers\n",
        "from .models import User, Trainer, Schedule, Booking\n",
        "\n",
        "\n",
        "class UserSerializer(serializers.ModelSerializer):\n",
        "    class Meta:\n",
        "        model = User\n",
        "        fields = ['id', 'username', 'email', 'role']\n",
        "\n",
        "\n",
        "class TrainerSerializer(serializers.ModelSerializer):\n",
        "    user = UserSerializer(read_only=True)\n",
        "\n",
        "    class Meta:\n",
        "        model = Trainer\n",
        "        fields = ['id', 'user', 'specialization', 'experience_years']\n",
        "\n",
        "\n",
        "class ScheduleSerializer(serializers.ModelSerializer):\n",
        "    trainer = TrainerSerializer(read_only=True)\n",
        "\n",
        "    class Meta:\n",
        "        model = Schedule\n",
        "        fields = ['id', 'trainer', 'date', 'time']\n",
        "\n",
        "\n",
        "class BookingSerializer(serializers.ModelSerializer):\n",
        "    client = UserSerializer(read_only=True)\n",
        "    schedule = ScheduleSerializer(read_only=True)\n",
        "\n",
        "    class Meta:\n",
        "        model = Booking\n",
        "        fields = ['id', 'client', 'schedule', 'created_at']\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "aLnP9WRvhJJg",
        "outputId": "1d34cfdd-5a2e-43b1-d0fa-19587c314116"
